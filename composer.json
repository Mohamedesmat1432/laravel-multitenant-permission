{
    "name": "elgaml/laravel-multi-tenancy-rbac",
    "description": "Advanced multi-tenancy and RBAC package for Laravel 12 API projects with Sanctum integration",
    "keywords": ["laravel", "multi-tenancy", "rbac", "api", "permissions", "sanctum"],
    "license": "MIT",
    "authors": [
        {
            "name": "Elgaml Team",
            "email": "team@elgaml.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "laravel/framework": "^12.0",
        "laravel/sanctum": "^4.0"
    },
    "require-dev": {
        "orchestra/testbench": "^9.0",
        "phpunit/phpunit": "^11.0"
    },
    "autoload": {
        "psr-4": {
            "Elgaml\\MultiTenancyRbac\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Elgaml\\MultiTenancyRbac\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Elgaml\\MultiTenancyRbac\\Providers\\MultiTenancyRbacServiceProvider"
            ],
            "aliases": {
                "Tenancy": "Elgaml\\MultiTenancyRbac\\Facades\\Tenancy",
                "Rbac": "Elgaml\\MultiTenancyRbac\\Facades\\Rbac"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
